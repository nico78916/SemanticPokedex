@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wdt: <https://dbpedia.org/ontology/>
@prefix ex: <http://exemple.org/ontology>

ex:Pokemon a rdfs:Class;
    rdfs:label "Pokemon";
    rdfs:SubClassOf wdt:Species.

# Class for Ability
ex:Ability a rdfs:Class ;
    rdfs:SubClassOf wdt:Skill;
    rdfs:label "Ability" ;
    rdfs:comment "A special skill or power that a Pokémon can have." .

# Class for Pokémon Type
ex:Elementary a rdfs:Class ;
    rdfs:label "Pokémon Type" ;
    rdfs:comment "An elemental type that defines the strengths and weaknesses of a Pokémon." .

ex:Generation a rdfs:Class ;
    rdfs:label "Generation" ;
    rdfs:comment "A generation of Pokémon games." .